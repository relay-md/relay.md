{
  "name": "relaymd",
  "version": "1.0.0",
  "scripts": {
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "sass --no-source-map backend/static/sass/main.scss:backend/static/css/main.css",
    "css-postcss": "postcss --output backend/static/css/main.css backend/static/css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "build": "npm run css-deploy && npm run js-deploy",
    "js-build": "esbuild backend/static/javascript/index.js --bundle --minify --outfile=backend/static/js/main.js"
  },
  "author": "Fabian Schuh",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/preset-env": "^7.23.7",
    "autoprefixer": "^10.4.16",
    "bulma": "^0.9.4",
    "bulma-extensions": "^6.2.7",
    "bulma-list": "^1.2.0",
    "esbuild": "^0.19.11",
    "highlight.js": "^11.9.0",
    "htmx.org": "^1.9.10",
    "js-yaml": "^4.1.0",
    "postcss-cli": "^11.0.0",
    "postcss-minify": "^1.1.0",
    "sass": "^1.69.7",
    "showdown": "^2.1.0"
  }
}
